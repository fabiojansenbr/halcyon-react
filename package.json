{
    "name": "halcyon-react",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "lint": "eslint ./src",
        "format": "prettier --write \"**/*.{js,jsx,json,scss,md,yml,html}\"",
        "analyze": "source-map-explorer build/static/js/main.*"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "1.2.8",
        "@fortawesome/free-brands-svg-icons": "5.5.0",
        "@fortawesome/free-solid-svg-icons": "5.5.0",
        "@fortawesome/react-fontawesome": "0.1.3",
        "availity-reactstrap-validation": "2.3.3",
        "axios": "0.18.0",
        "bootswatch": "4.1.3",
        "history": "4.7.2",
        "izitoast": "1.4.0",
        "jwt-decode": "2.2.0",
        "prop-types": "15.6.2",
        "qrcode.react": "0.8.0",
        "react": "16.6.1",
        "react-dom": "16.6.1",
        "react-redux": "5.1.0",
        "react-router": "4.3.1",
        "react-router-dom": "4.3.1",
        "react-scripts": "2.1.1",
        "react-social-login": "3.4.2",
        "reactstrap": "6.5.0",
        "redux": "4.0.1",
        "redux-axios-middleware": "4.0.0",
        "redux-thunk": "2.3.0"
    },
    "devDependencies": {
        "husky": "1.1.3",
        "lint-staged": "8.0.4",
        "prettier": "1.15.1",
        "source-map-explorer": "1.6.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx,json,css,md,yml,html}": [
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
